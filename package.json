{
  "name": "split-guide",
  "version": "0.0.0-semantically-released",
  "description": "A tool to help generate code for workshop repositories",
  "main": "dist/index.js",
  "scripts": {
    "start": "nps",
    "test": "nps test",
    "commitmsg": "opt --in commit-msg --exec \"validate-commit-msg\"",
    "precommit": "opt --in pre-commit --exec \"npm start validate\""
  },
  "bin": {
    "split-guide": "dist/bin/index.js"
  },
  "files": [
    "dist"
  ],
  "keywords": [],
  "author": "Kent C. Dodds <kent@doddsfamily.us> (http://kentcdodds.com/)",
  "license": "MIT",
  "dependencies": {
    "glob": "^7.0.6",
    "mkdirp": "^0.5.1",
    "pify": "^2.3.0",
    "rimraf": "^2.5.4",
    "yargs": "5.0.0"
  },
  "devDependencies": {
    "all-contributors-cli": "^3.0.6",
    "babel-cli": "^6.14.0",
    "babel-jest": "^15.0.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-2": "^6.13.0",
    "babel-register": "^6.14.0",
    "codecov": "^1.0.1",
    "commitizen": "^2.8.6",
    "cz-conventional-changelog": "^1.2.0",
    "directory-tree": "^1.1.0",
    "eslint": "^3.4.0",
    "eslint-config-kentcdodds": "^10.1.0",
    "husky": "^0.11.7",
    "jest-cli": "^15.1.1",
    "opt-cli": "^1.5.1",
    "p-s": "^3.0.1",
    "semantic-release": "^4.3.5",
    "spawn-command": "0.0.2-1",
    "validate-commit-msg": "^2.8.0"
  },
  "eslintConfig": {
    "extends": [
      "kentcdodds",
      "kentcdodds/jest"
    ],
    "rules": {
      "global-require": 0,
      "import/newline-after-import": 0
    }
  },
  "jest": {
    "cacheDirectory": "./node_modules/.cache",
    "testEnvironment": "node",
    "testPathDirs": [
      "src"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kentcdodds/split-guide.git"
  },
  "bugs": {
    "url": "https://github.com/kentcdodds/split-guide/issues"
  },
  "homepage": "https://github.com/kentcdodds/split-guide#readme"
}
